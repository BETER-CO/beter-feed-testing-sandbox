{
  "Serilog": {
    "MinimumLevel": {
      "Override": {
        "Microsoft.AspNetCore.SignalR": "Debug",
        "Microsoft.AspNetCore.Http.Connections": "Debug",
        "FeedService.Module.Consumers.TimeTableConsumer": "Error",
        "FeedService.Module.BookingSubscriptionManager": "Warning",
        "Microsoft": "Warning",
        "System": "Warning"
      }
    }
  },
  "Logger": {
    "MinimumLogLevel": "Information",
    "Sinks": {
      "Console": {
        "LogLevel": "Debug"
      },
      "Elastic": {
        "LogLevel": "Information",
        "ElasticSearchUrl": "http://ingest-es.bsrv.tech:9200/"
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information",
      "Microsoft.AspNetCore.SignalR": "Trace",
      "Microsoft.AspNetCore.Http.Connections": "Trace"
    }
  },
  "Metrics": {
    "ScrapeEndpointPath": "/metrics",
    "UriPrefixes": [ "http://+:9464/" ]
  },

  "Messaging": {
    "ConsumerConfig": {
      "BootstrapServers": "localhost:9092",
      "SslKeyLocation": null,
      "SslCertificateLocation": null,
      "SecurityProtocol": 0,
      "AllowAutoCreateTopics": true,
      "AutoOffsetReset": "Earliest",
      "GroupId": "feed-emulator:test"
    },
    "Topics": {
      "TestingTools": "testing-tool-generator-local"
    }
  },
  "SignalR": {
    "FeedTimeTableEndpoint": "/time_table",
    "FeedTradingEndpoint": "/trading",
    "FeedScoreboardEndpoint": "/scoreboard",
    "FeedIncidentEndpoint": "/incident",
    "TtlForHeartbetRecord": "00:00:05"
  }
}